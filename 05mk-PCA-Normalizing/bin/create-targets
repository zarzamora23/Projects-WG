#!/bin/bash
##
## DESCRIPTION:
##      Script to print required mk targets to STDOUT.
## USAGE:
##      bin/create-targets

# Obtain depth of coverage file coresponding to one or several samples.
#
find -L data/ \
        -name '*.filt_cent.RD.txt' \
| sed -r \
        -e 's#^data/#results/#' \
        -e 's#.filt_cent.RD.txt$#.PCA_normalized.txt#g' 

 
